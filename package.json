{
  "name": "varintes",
  "version": "1.0.3",
  "description": "Unsigned Varint encoding and decoding, exposed as ESModule",
  "keywords": [
    "unsigned-varint",
    "varint"
  ],
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukstv/varintes.git"
  },
  "bugs": {
    "url": "https://github.com/ukstv/varintes/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./decode": "./dist/decode.js",
    "./decode-pack": "./dist/decode-pack.js",
    "./encode": "./dist/encode.js",
    "./encode-pack": "./dist/encode-pack.js",
    "./encoding-length": "./dist/encoding-length.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/tsm ./node_modules/uvu/bin.js . .+\\.test\\.ts",
    "build": "./node_modules/.bin/tsc --project tsconfig.build.json",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "tsm": "^2.2.2",
    "typescript": "^4.8.2",
    "uint8arrays": "^3.1.0",
    "uvu": "^0.5.6"
  }
}
